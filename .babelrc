{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "usage",
            "corejs": { "version": "3.8", "proposals": true },
            "targets": {
                "browsers": [">1%", "last 3 version", "not ie < 10"]
            },
            "modules": false,
            "debug": false
        }]
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {}]
    ]
}
